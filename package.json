{
  "name": "startrek-names",
  "version": "0.0.0-semantically-released",
  "description": "A Node.js module to randomly generate StarTrek character names.",
  "main": "index.js",
  "bin": {
    "startrek-names": "bin/cli.js"
  },
  "scripts": {
    "commit": "git-cz",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage":"cat ./coverage/lcov.info | codecov",
    "test": "mocha test -w",
    "test:single": "istanbul cover -x *.test.js _mocha -- -R spec test",
    "install": "echo \"Thank you for installing!\"",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "keywords": [
    "star",
    "trek",
    "names",
    "characters",
    "random",
    "generate",
    "startrek",
    "spock",
    "kirk",
    "names"
  ],
  "author": "Aman Mittal <amandeepmittal@live.com> (http://amandeepmittal.github.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amandeepmittal/startrek-names/issues"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "codecov.io": "0.1.6",
    "commitizen": "2.8.5",
    "cz-conventional-changelog": "1.1.7",
    "ghooks": "1.3.2",
    "istanbul": "0.4.4",
    "mocha": "2.5.3",
    "semantic-release": "^4.3.5"
  },
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/amandeepmittal/startrek-names.git"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test:single && npm run check-coverage"
    }
  }
}
