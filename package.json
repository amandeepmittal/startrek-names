{
  "name": "startrek-names",
  "version": "2.0.0",
  "description": "A Node.js module to randomly generate StarTrek character names.",
  "main": "dist/index.js",
  "bin": {
    "startrek-names": "bin/cli.js"
  },
  "scripts": {
    "commit": "git-cz",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "watch:test": "npm t -- -w",
    "test": "mocha test --compilers js:babel-register",
    "cover": "nyc npm t",
    "install": "echo \"Thank you for installing!\"",
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel --copy-files --out-dir dist lib",
    "build:umd": "webpack --output-filename index.umd.js",
    "build:umd.min": "webpack --output-filename index.umd.min.js -p",
  },
  "keywords": [
    "star",
    "trek",
    "names",
    "characters",
    "random",
    "generate",
    "startrek",
    "spock",
    "kirk",
    "names"
  ],
  "files": [
    "dist",
    "readme.md"
  ],
  "author": "Aman Mittal <amandeepmittal@live.com> (http://amandeepmittal.github.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amandeepmittal/startrek-names/issues"
  },
  "devDependencies": {
    "babel-cli": "6.11.4",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-stage-2": "6.13.0",
    "babel-register": "6.9.0",
    "chai": "3.5.0",
    "codecov.io": "0.1.6",
    "commitizen": "2.8.5",
    "cz-conventional-changelog": "1.1.7",
    "ghooks": "1.3.2",
    "istanbul": "0.4.4",
    "json-loader": "0.5.4",
    "mocha": "2.5.3",
    "npm-run-all": "2.3.0",
    "nyc": "6.4.0",
    "rimraf": "2.5.4",
    "webpack": "1.13.1"
  },
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/amandeepmittal/startrek-names.git"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run cover && npm run check-coverage"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}
